{
    "manifest_version": 2,
    "name": "Pi-Hole Plus",
    "description": "A Chrome extension that enables users to easily manage their Pi-Hole from the browser toolbar.",
    "version": "1.0.0",
  
    "browser_action": {
      "default_popup": "index.html"
    },
    "permissions": [
      "activeTab"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "web_accessible_resources": [
      "index.html",
      "ext.js",
      "style.css"
    ]
}
  